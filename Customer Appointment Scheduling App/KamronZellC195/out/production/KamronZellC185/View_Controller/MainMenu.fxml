<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="600.0" prefWidth="860.0" style="-fx-background-color: a;" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="View_Controller.MainMenuController">
   <children>
      <TableView fx:id="scheduledAptTable" layoutX="-1.0" layoutY="360.0" prefHeight="239.0" prefWidth="866.0">
        <columns>
          <TableColumn fx:id="aptIDCol" prefWidth="86.0" text="Apt ID" />
          <TableColumn fx:id="customerCol" prefWidth="79.0" text="Customer " />
            <TableColumn fx:id="typeCol" prefWidth="89.0" text="Type" />
            <TableColumn fx:id="scheduledTimeCol" prefWidth="124.0" text="Scheduled Time" />
        </columns>
      </TableView>
      <Text layoutX="14.0" layoutY="309.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Scheduled Appointments">
         <font>
            <Font size="23.0" />
         </font></Text>
      <RadioButton layoutX="454.0" layoutY="296.0" mnemonicParsing="false" onAction="#ViewMonthRadioButton" text="View by Month">
         <toggleGroup>
            <ToggleGroup fx:id="view" />
         </toggleGroup>
      </RadioButton>
      <RadioButton layoutX="581.0" layoutY="296.0" mnemonicParsing="false" onAction="#viewWeekRadioButton" text="View by Week" toggleGroup="$view" />
      <RadioButton layoutX="706.0" layoutY="296.0" mnemonicParsing="false" onAction="#viewAllRadioButton" selected="true" text="View All" toggleGroup="$view" />
      <HBox layoutY="138.0" prefHeight="100.0" prefWidth="823.0">
         <children>
            <ButtonBar prefHeight="100.0" prefWidth="830.0">
              <buttons>
                <Button mnemonicParsing="false" onAction="#addCustButton" text="Add Customer" />
                  <Button layoutX="758.0" layoutY="48.0" mnemonicParsing="false" onAction="#modCustButton" text="Modify Customer" />
                  <Button layoutX="758.0" layoutY="48.0" mnemonicParsing="false" onAction="#addAptButton" text="Add Appointment" />
                  <Button layoutX="673.0" layoutY="48.0" mnemonicParsing="false" onAction="#modAptButton" text="Modify Appointment" />
                  <Button layoutX="503.0" layoutY="48.0" mnemonicParsing="false" onAction="#reportButton" text="Reports" />
              </buttons>
               <HBox.margin>
                  <Insets left="50.0" right="50.0" />
               </HBox.margin>
            </ButtonBar>
         </children>
      </HBox>
      <Button fx:id="exitProgramButton" layoutX="771.0" layoutY="14.0" mnemonicParsing="false" onAction="#exitButton" text="EXIT" />
      <Label fx:id="timeZoneLabel" layoutX="32.0" layoutY="18.0" text="Current TimeZone" />
      <Label layoutX="356.0" layoutY="84.0" text="Main Menu">
         <font>
            <Font size="30.0" />
         </font>
      </Label>
      <DatePicker fx:id="datePicker" layoutX="677.0" layoutY="328.0" />
   </children>
</AnchorPane>
